

{% extends 'base-back.html.twig' %}
{%block title %} AJOUTER TYPE SEANCE  {% endblock %}


{% block body %}
   <div class="container">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Ajouter type de s√©ance</h4>
                </div>
                <div class="card-body">
                    {{ form_start(form, {'attr': {'class': 'form', 'novalidate': 'novalidate'}}) }}
                    <div class="form-group row">
                        <label for="nom" class="col-md-4 col-form-label text-md-right">Nom de type:</label>
                        <div class="col-md-6">
                            {{ form_row(form.nom_type, {'attr': {'class': 'form-control'}}) }}
                        </div>
                    </div>
                   
                   
                    <div class="form-group row mb-0">
                        <div class="col-md-6 offset-md-4">
                            {{ form_row(form.Ajouter, {'attr': {'class': 'btn btn-primary'}}) }}
                        </div>
                    </div>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>
</div>

{% endblock %}




